{
  "name": "Mercury",
  "version": "0.0.0",
  "description": "Wikia Web Application",
  "main": "server.js",
  "scripts": {
    "dev": "rm -rf www && gulp --nosync",
    "local": "rm -rf www && gulp",
    "start": "npm run build && node dist/server/app/server.js",
    "test": "npm run build-test && npm run test-front && npm run test-server",
    "coverage": "npm run build-coverage && npm run test-front && npm run test-server",
    "test-front": "gulp karma",
    "test-server": "gulp node-test",
    "linter": "eslint .",
    "build": "rm -rf dist && npm-run-all build-common build-main build-server",
    "build-common": "rm -rf dist/front/common && gulp build-common",
    "build-main": "rm -rf dist/front/main && gulp build-common && cd front/main && ember build",
    "build-server": "rm -rf dist/server && gulp build-server"
  },
  "repository": "git://github.com/Wikia/mercury.git",
  "license": "MIT",
  "homepage": "https://github.com/Wikia/mercury",
  "private": true,
  "dependencies": {
    "bluebird": "2.10.2",
    "boom": "2.7.0",
    "bunyan": "1.3.4",
    "bunyan-syslog": "0.3.1",
    "bunyan-prettystream": "0.1.3",
    "deep-extend": "0.3.3",
    "handlebars": "2.0.0",
    "hapi": "8.4.0",
    "hapi-i18next": "2.2.0",
    "hoek": "2.12.0",
    "i18next": "1.7.10",
    "newrelic": "1.18.1",
    "wreck": "6.3.0"
  },
  "devDependencies": {
    "babel-eslint": "4.1.6",
    "babel-plugin-rename-define": "Wikia/babel-plugin-rename-define#v2.0.0",
    "babel-plugin-transform-es2015-modules-amd": "6.3.13",
    "babel-preset-es2015": "6.3.13",
    "browser-sync": "2.10.1",
    "ember-template-compiler": "1.9.0-alpha",
    "eslint": "1.10.3",
    "flipcss": "0.2.8",
    "glob": "4.5.3",
    "gulp": "3.9.0",
    "gulp-autoprefixer": "2.3.1",
    "gulp-babel": "6.1.1",
    "gulp-cached": "1.1.0",
    "gulp-concat": "2.6.0",
    "gulp-declare": "0.3.0",
    "gulp-develop-server": "0.2.5",
    "gulp-expect-file": "0.0.7",
