<div id="wikiContainer" class="wiki-container">
	{{article-wrapper
		commentsPage=commentsPage
		model=model
		siteName=siteName
		wikiPageHeaderSubtitle=(i18n 'blog-page.subtitle')
		edit=(action 'edit' target=article)
		openLightbox=(action 'openLightbox' target=application)
		toggleDrawer=(action 'toggleDrawer' target=application)
	}}
</div>
