{{#if isLoading}}
  {{wds-spinner
    active=isLoading
    isBlock=true
    overlay=false
  }}
{{else}}
  {{#if model.membersGrouped}}
    {{category-members-grouped
      membersGrouped=model.membersGrouped
    }}
  {{/if}}
  {{#if model.hasPagination}}
    {{category-pagination
      firstPageUrl=model.firstPageUrl
      lastPageKey=model.lastPageKey
      lastPageUrl=model.lastPageUrl
      loadFrom=(action "loadFrom")
      nextPageKey=model.nextPageKey
      nextPageUrl=model.nextPageUrl
      prevPageKey=model.prevPageKey
      prevPageUrl=model.prevPageUrl
    }}
  {{/if}}
{{/if}}
{{alert-notifications alerts=alertNotifications}}
