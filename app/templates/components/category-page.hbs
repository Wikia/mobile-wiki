<p class="category-page__total-number">{{i18n 'category-page.total-number' totalNumberOfItems=model.totalNumberOfMembers}}</p>
{{#if isLoading}}
  {{wds-spinner
    active=isLoading
    isBlock=true
    overlay=false
  }}
{{else}}
  {{category-members-grouped
    model=model.members
  }}
  {{#if model.hasPagination}}
    {{category-pagination
      loadFrom=(action "loadFrom")
      model=model.pagination
    }}
  {{/if}}
{{/if}}
{{alert-notifications alerts=alertNotifications}}
