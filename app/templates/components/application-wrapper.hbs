{{#if fullPage}}
  {{outlet}}
{{else}}
  {{site-head
    globalNavigation=globalNavigation
    communityHeader=communityHeader
    contentLanguage=contentLanguage
    isSearchPage=isSearchPage
    shadow=siteHeadShadow
    themeBar=themeBar
    offset=ads.siteHeadOffset
  }}
  <div class="page-wrapper">
    {{outlet}}
  </div>
  {{wikia-in-your-lang}}
  {{#if globalFooter}}
    {{wds-global-footer
      model=globalFooter
      track=(action track)
    }}
    {{global-footer-bottom}}
  {{/if}}
{{/if}}
{{lightbox-wrapper}}
{{alert-notifications alerts=alertNotifications}}
{{autologin-frame}}
{{autologin-passive-frame}}