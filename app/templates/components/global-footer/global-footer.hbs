<footer class="wds-global-footer">
	{{#if model.header}}
		<h2 class="wds-global-footer__header">
			<a href="{{model.header.href}}" {{action 'track' model.header.tracking_label preventDefault=false}} title="{{model.header.title.value}}">
				{{svg model.header.image-data.name class='wds-global-footer__header-logo'}}
			</a>
		</h2>
	{{/if}}
	<div class="wds-global-footer__main">
		<div class="wds-global-footer__fandom-sections">
			{{global-footer/global-footer-section model=model.fandom_overview name='fandom-overview' parentName='fandom' track=(action 'track')}}
			{{global-footer/global-footer-section model=model.follow_us name='follow-us' parentName='fandom' track=(action 'track')}}
		</div>
		<div class="wds-global-footer__wikia-sections">
			{{global-footer/global-footer-section model=model.company_overview name='company-overview' parentName='wikia' track=(action 'track')}}
			{{global-footer/global-footer-section model=model.site_overview name='site-overview' parentName='wikia' track=(action 'track')}}
			{{global-footer/global-footer-section model=model.community name='community' parentName='wikia' track=(action 'track')}}
			{{global-footer/global-footer-section model=model.create_wiki name='create-wiki' parentName='wikia' track=(action 'track')}}
			{{global-footer/global-footer-section model=model.community_apps name='community-apps' parentName='wikia' track=(action 'track')}}
			{{global-footer/global-footer-section model=model.advertise name='advertise' parentName='wikia' track=(action 'track')}}
		</div>
	</div>
	{{global-footer/global-footer-bottom-bar model=model track=(action 'track')}}
</footer>
