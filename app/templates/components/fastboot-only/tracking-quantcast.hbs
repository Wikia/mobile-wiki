<script src="{{src}}" async></script>
<script>
	(function () {
		window.trackQuantcastPageView = function () {
			window._qevents = window._qevents || [];

			{{! without this quantserve does not want to track 2+ page view (ADEN-4803) }}
			if (window.__qc) {
				window.__qc.qpixelsent = [];
			}

			window._qevents.push({
				qacct: '{{account}}',
				labels: 'Category.MobileWeb.Mercury'
			});

			console.info('Track pageView: Quantserve');
		};

		window.trackQuantcastPageView();
	})();
</script>
<noscript style="display: none">
	<img src="//pixel.quantserve.com/pixel/{{account}}.gif?labels=Category.MobileWeb.Mercury" height="1" width="1" alt="">
</noscript>
