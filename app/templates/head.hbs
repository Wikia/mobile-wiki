{{! Tags set once (static head tags) }}
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1, minimal-ui">

{{#if model.themeColor}}
	<meta name="theme-color" content="{{model.themeColor}}">
{{/if}}

<link rel="preconnect" href="https://vignette.wikia.nocookie.net">

{{#unless model.noExternals}}
	<link rel="dns-prefetch" href="http://edge.quantserve.com">
	<link rel="dns-prefetch" href="http://secure-dcr.imrworldwide.com/">
	<link rel="dns-prefetch" href="http://b.scorecardresearch.com">
	<link rel="dns-prefetch" href="https://script.ioam.de/iam.js">

	{{#if model.gaUrl}}
		<link rel="dns-prefetch" href="{{model.gaUrl}}">
	{{/if}}
{{/unless}}

{{#if model.favicon}}
	<link rel="shortcut icon" href="{{model.favicon}}">
{{/if}}

{{#if model.appleTouchIcon}}
	<link rel="apple-touch-icon" href="{{model.appleTouchIcon.url}}" sizes="{{model.appleTouchIcon.size}}">
{{/if}}

{{#if model.amphtml}}
	<link rel="amphtml" href="{{model.amphtml}}">
{{/if}}

{{! Tags updated on each transition (dynamic head tags) }}
{{#if model.htmlTitle}}
	<title>{{model.htmlTitle}}</title>
{{/if}}

{{#if model.description}}
	<meta name="description" content="{{model.description}}">
{{/if}}

{{#if model.keywords}}
	<meta name="keywords" content="{{model.keywords}}">
{{/if}}

{{#if model.robots}}
	<meta name="robots" content="{{model.robots}}">
{{/if}}

{{#if model.appleItunesApp}}
	<meta name="apple-itunes-app" content="{{model.appleItunesApp}}">
{{/if}}

{{#if model.canonical}}
	<link rel="canonical" href="{{model.canonical}}">
{{/if}}

{{#if model.next}}
	<link rel="next" href="{{model.next}}">
{{/if}}

{{#if model.prev}}
	<link rel="prev" href="{{model.prev}}">
{{/if}}

{{! Open Graph Tags }}
<meta property="og:type" content="{{model.type}}">

{{#unless model.isMainPage}}
	<meta property="og:site_name" content="{{model.siteName}}">
{{/unless}}

<meta property="og:title" content="{{model.title}}">

{{#if model.description}}
	<meta property="og:description" content="{{model.description}}">
{{/if}}

<meta property="og:url" content="{{model.canonical}}">

{{#if model.pageImage}}
	<meta property="og:image" content="{{model.pageImage}}">
{{/if}}

{{#if model.facebookAppId}}
	<meta property="fb:app_id" content="{{model.facebookAppId}}" prefix="fb: http://www.facebook.com/2008/fbml">
{{/if}}

<script id="liftigniter-metadata" type="application/json">{"noIndex":"true"}</script>

{{#if model.lazyCss}}
	{{!- These styles will be added to head on front-end to lazy load them-}}
	{<link rel="stylesheet" href="/mobile-wiki/assets/lazy.css">
{{/if}}
