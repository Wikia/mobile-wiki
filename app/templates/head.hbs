{{#if model.appleTouchIcon}}
  <link
    rel="apple-touch-icon"
    href={{model.appleTouchIcon.url}}
    sizes={{model.appleTouchIcon.size}}
  >
{{/if}}

{{#if model.amphtml}}
  <link rel="amphtml" href={{model.amphtml}}>
{{/if}}

{{! Tags updated on each transition (dynamic head tags) }}
{{#if model.htmlTitle}}
  <title>{{model.htmlTitle}}</title>
{{/if}}

{{#if model.description}}
  <meta name="description" content={{model.description}}>
{{/if}}

{{#if model.keywords}}
  <meta name="keywords" content={{model.keywords}}>
{{/if}}

{{#if model.robots}}
  <meta name="robots" content={{model.robots}}>
{{/if}}

{{#if model.appleItunesApp}}
  <meta name="apple-itunes-app" content={{model.appleItunesApp}}>
{{/if}}

{{#if model.canonical}}
  <link rel="canonical" href={{model.canonical}}>
{{/if}}

{{#if model.next}}
  <link rel="next" href={{model.next}}>
{{/if}}

{{#if model.prev}}
  <link rel="prev" href={{model.prev}}>
{{/if}}

{{#each-in model.hreflangLinks as |language url|}}
  <link
    rel="alternate"
    hreflang={{language}}
    href={{url}}
  >
{{/each-in}}

{{! Open Graph start }}
{{#if model.type}}
  <meta property="og:type" content={{model.type}}>
{{/if}}
{{#unless model.isMainPage}}
  <meta property="og:site_name" content={{model.siteName}}>
{{/unless}}
{{#if model.title}}
  <meta property="og:title" content={{model.title}}>
{{/if}}
{{#if model.description}}
  <meta property="og:description" content={{model.description}}>
{{/if}}
{{#if model.canonical}}
  <meta property="og:url" content={{model.canonical}}>
{{/if}}
{{#if model.pageImage}}
  <meta property="og:image" content={{model.pageImage}}>
{{/if}}
{{! Open Graph end }}

{{! Facebook }}
{{#if model.facebookAppId}}
  <meta
    property="fb:app_id"
    content={{model.facebookAppId}}
    prefix="fb: http://www.facebook.com/2008/fbml"
  >
{{/if}}

{{! Twitter start }}
<meta name="twitter:card" content="summary">
<meta name="twitter:site" content={{model.twitterSite}}>
{{#if model.canonical}}
  <meta name="twitter:url" content={{model.canonical}}>
{{/if}}
{{#if model.twitterTitle}}
  <meta name="twitter:title" content={{model.twitterTitle}}>
{{/if}}
{{#if model.twitterDescription}}
  <meta name="twitter:description" content={{model.twitterDescription}}>
{{/if}}
{{! Twitter end }}

{{! We use both meta tags below instead of shoebox to have the video related data accessible ASAP (before ember boots)}}
{{#if model.isDedicatedForArticle}}
  <meta name="featured-video:is-dedicated-for-article" content={{model.isDedicatedForArticle}}>
{{/if}}

{{#if model.playerImpressionsPerSession}}
  <meta name="featured-video:impressions-per-session" content={{model.playerImpressionsPerSession}}>
{{/if}}

{{#if model.videoBridgeCountries}}
  <meta name="featured-video:video-bridge-countries" content={{model.videoBridgeCountries}}>
{{/if}}
{{! end of video tags}}

{{#if model.lazyCss}}
  {{!- These styles will be added to head on front-end to lazy load them-}}
  <link rel="stylesheet" href="/mobile-wiki/assets/lazy.css">
{{/if}}

{{#if model.shouldLoadDarkCss}}
  {{!- Styles for dark theme added only in the mobile app view-}}
  <link rel="stylesheet" href="/mobile-wiki/assets/dark.css">
{{/if}}
