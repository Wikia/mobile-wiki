{{! Tags set once (static head tags) }}
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1, minimal-ui">

<meta name="theme-color" content="#002a32">

<link rel="preconnect" href="https://vignette.wikia.nocookie.net">
{{! fallback to dns-prefetch for iOS where preconnect isn't supported yet}}
<link rel="dns-prefetch" href="https://vignette.wikia.nocookie.net">

{{#unless model.noExternals}}
	<link rel="dns-prefetch" href="http://edge.quantserve.com">
	<link rel="dns-prefetch" href="http://secure-dcr.imrworldwide.com/">
	<link rel="dns-prefetch" href="http://b.scorecardresearch.com">
	<link rel="dns-prefetch" href="https://script.ioam.de/iam.js">
	<link rel="preconnect" href="{{model.cdnRootUrl}}">
	<link rel="dns-prefetch" href="{{model.cdnRootUrl}}">
	<link rel="preconnect" href="https://beacon.wikia-services.com">
	<link rel="dns-prefetch" href="https://beacon.wikia-services.com">

	{{! Ads header bidding providers}}
	{{! a9}}
	<link rel="preconnect" href="//c.amazon-adsystem.com">
	<link rel="dns-prefetch" href="//c.amazon-adsystem.com">
	{{! rubicon}}
	<link rel="preconnect" href="//fastlane.rubiconproject.com">
	<link rel="dns-prefetch" href="//fastlane.rubiconproject.com">
	<link rel="preconnect" href="//fastlane-adv.rubiconproject.com">
	<link rel="dns-prefetch" href="//fastlane-adv.rubiconproject.com">
	{{! pubmatic}}
	<link rel="preconnect" href="//ads.pubmatic.com">
	<link rel="dns-prefetch" href="//ads.pubmatic.com">
	<link rel="preconnect" href="//gads.pubmatic.com">
	<link rel="dns-prefetch" href="//gads.pubmatic.com">
	{{! aol}}
	<link rel="preconnect" href="//adserver-us.adtech.advertising.com">
	<link rel="dns-prefetch" href="//adserver-us.adtech.advertising.com">
	{{! oneMobile}}
	<link rel="preconnect" href="//hb.nexage.com">
	<link rel="dns-prefetch" href="//hb.nexage.com">
	{{! indexExchange}}
	<link rel="preconnect" href="http://as.casalemedia.com">
	<link rel="dns-prefetch" href="http://as.casalemedia.com">
	{{! appnexus}}
	<link rel="preconnect" href="//ib.adnxs.com">
	<link rel="dns-prefetch" href="//ib.adnxs.com">
	{{! beachfront}}
	<link rel="preconnect" href="//reachms.bfmio.com">
	<link rel="dns-prefetch" href="//reachms.bfmio.com">
	{{! openx}}
	<link rel="preconnect" href="//wikia-d.openx.net">
	<link rel="dns-prefetch" href="//wikia-d.openx.net">
	{{! googletag gpt}}
	<link rel="preconnect" href="//www.googletagservices.com">
	<link rel="dns-prefetch" href="//www.googletagservices.com">
	{{! kargo}}
	<link rel="preconnect" href="//krk.kargo.com">
	<link rel="dns-prefetch" href="//krk.kargo.com">

	{{#if model.gaUrl}}
		<link rel="dns-prefetch" href="{{model.gaUrl}}">
	{{/if}}
{{/unless}}

{{#if model.favicon}}
	<link rel="shortcut icon" href="{{model.favicon}}">
{{/if}}

{{#if model.appleTouchIcon}}
	<link rel="apple-touch-icon" href="{{model.appleTouchIcon.url}}" sizes="{{model.appleTouchIcon.size}}">
{{/if}}

{{#if model.amphtml}}
	<link rel="amphtml" href="{{model.amphtml}}">
{{/if}}

{{! Tags updated on each transition (dynamic head tags) }}
{{#if model.htmlTitle}}
	<title>{{model.htmlTitle}}</title>
{{/if}}

{{#if model.description}}
	<meta name="description" content="{{model.description}}">
{{/if}}

{{#if model.keywords}}
	<meta name="keywords" content="{{model.keywords}}">
{{/if}}

{{#if model.robots}}
	<meta name="robots" content="{{model.robots}}">
{{/if}}

{{#if model.appleItunesApp}}
	<meta name="apple-itunes-app" content="{{model.appleItunesApp}}">
{{/if}}

{{#if model.canonical}}
	<link rel="canonical" href="{{model.canonical}}">
{{/if}}

{{#if model.next}}
	<link rel="next" href="{{model.next}}">
{{/if}}

{{#if model.prev}}
	<link rel="prev" href="{{model.prev}}">
{{/if}}

{{! Open Graph Tags }}
<meta property="og:type" content="{{model.type}}">

{{#unless model.isMainPage}}
	<meta property="og:site_name" content="{{model.siteName}}">
{{/unless}}

<meta property="og:title" content="{{model.title}}">

{{#if model.description}}
	<meta property="og:description" content="{{model.description}}">
{{/if}}

<meta property="og:url" content="{{model.canonical}}">

{{#if model.pageImage}}
	<meta property="og:image" content="{{model.pageImage}}">
{{/if}}

{{#if model.facebookAppId}}
	<meta property="fb:app_id" content="{{model.facebookAppId}}" prefix="fb: http://www.facebook.com/2008/fbml">
{{/if}}

<script id="liftigniter-metadata" type="application/json">{"noIndex":"true"}</script>

{{#if model.lazyCss}}
	{{!- These styles will be added to head on front-end to lazy load them-}}
	<link rel="stylesheet" href="/mobile-wiki-assets/assets/lazy.css">
{{/if}}
