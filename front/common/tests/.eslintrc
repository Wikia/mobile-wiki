{
  "env": {
    "node": true,
    "embertest": true,
    "es6": false
  },
  "globals": {
    "sinon": true,
    "QUnit": true,
    "test": true,
    "moduleForComponent": true,
    "throws": true
  },
  "rules": {
    "arrow-parens": 0,
    "arrow-spacing": 0,
    "constructor-super": 0,
    "generator-star-spacing": 0,
    "no-class-assign": 0,
    "no-const-assign": 0,
    "no-dupe-class-members": 0,
    "no-new": 0,
    "no-this-before-super": 0,
    "no-undefined": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "prefer-template": 0,
    "require-yield": 0
  }
}
