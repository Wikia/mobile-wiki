{{#discussion-entities-list-layout
	applyFilters=(action 'applyFilters')
	hasContentError=true
	model=model.data
	updateCategories=(action 'updateCategories')
}}
	{{#if model.error.isNotFound}}
		{{discussion-reported-not-found-error}}
	{{else}}
		{{discussion-connection-error
			error=model.error
		}}
	{{/if}}
{{/discussion-entities-list-layout}}
