{{infobox-builder
	activeItem=model.itemInEditMode
	isDirty=isDirty
	state=model.infoboxState
	theme=model.theme
	title=model.title
	initialTitle=model.initialTitle
	addItem=(action 'addItem')
	cancelAction=(action 'cancel')
	editRowItem=(action 'editRowItem')
	editSectionHeaderItem=(action 'editSectionHeaderItem')
	editTitleItem=(action 'editTitleItem')
	getDiffArray=(action 'getDiffArray')
	goToSourceEditor=(action 'goToSourceEditor')
	removeItem=(action 'removeItem')
	reorder=(action 'updateInfoboxStateOrder')
	saveAction=(action 'save')
	getTemplateExistsAction=(action 'getTemplateExists')
	setEditItem=(action 'setEditItem')
}}
