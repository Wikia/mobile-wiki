{{#unless noAds}}
	<div id="InvisibleHighImpactWrapper" class="hidden invisible-high-impact-wrapper">
		<div class="background"></div>
		<a class="close">
			{{svg 'close' role='img' class='icon close-button'}}
		</a>
		<div class="label">{{i18n 'app.advertisement-label'}}</div>

		<div id="{{name}}" class="ad-slot hidden {{nameLowerCase}}"></div>
	</div>
{{/unless}}
