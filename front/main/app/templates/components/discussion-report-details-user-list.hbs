<a href="#" class="close" {{action this.attrs.close bubbles=false}}>
	{{svg 'close' class="fill-theme-color" viewBox='0 0 18 18'}}
</a>
{{i18n 'main.reports_number' count=reportDetails.count ns='discussion'}}
<ul>
{{#each reportDetails.users as |user|}}
	<li>
		{{user-avatar
			avatarUrl=user.avatarUrl
			displayProfileName=true
			username=user.name
		}}
	</li>
{{/each}}
</ul>
