{{#if users.length}}
	<div class="title">{{i18n 'article.top-contributors-label'}}</div>
	<ul>
		{{#each users as |user|}}
			<li>
				<a href="{{unbound user.url}}" {{action 'trackClick' 'contributors' 'open-user' preventDefault=false}}>
					{{thumbnail user.avatar width=100 height=100 mode=user.thumbMode alt=user.name}}
				</a>
			</li>
		{{/each}}
	</ul>
{{/if}}
