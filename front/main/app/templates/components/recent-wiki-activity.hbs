<header class="sub-head">
	<div class="sub-head--title sub-head--text-only">{{i18n 'main.title' ns='recent-wiki-activity'}}</div>
</header>

<div id="recent-changes">
	{{#each model.recentChanges as |change|}}
		{{recent-change model=change id=change.id newId=change.revid rc=rc upvote=addRevisionUpvote}}
	{{/each}}
</div>
