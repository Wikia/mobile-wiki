<div class="wiki-container">
	<h2 class="wiki-title">{{link-to siteName 'wiki-page' mainPageTitle alt=siteName trackingCategory='wikiname'}}</h2>
	<section class="wiki-page-error">
		<h1 class="wiki-page-title">Error 404</h1>
		<div>{{i18n 'app.page-not-found-error'}}</div>
		<a {{action 'toggleDrawer' true}}>{{i18n 'app.page-not-found-message'}}</a>
	</section>
	{{wikia-footer}}
</div>
