{{#unless queryParams.noexternals}}
	<!-- Universal Analytics -->
	<script>
		window['GoogleAnalyticsObject'] = 'ga';

		// create initial ga() function
		window['ga'] = window['ga'] || function () {
			(window['ga'].q = window['ga'].q || []).push(arguments);
		};

		// Sets the time (as an integer) this tag was executed.  Used for timing hits.
		window['ga'].l = 1 * new Date();

		$script(['{{server.gaUrl}}'], 'ua');

		$script.ready(['ua', 'optimizely'], function() {
			$.getJSON('/wikia.php?controller=MercuryApi&method=getWikiVariables', function(data) {
				console.info('Tracking PageView: DUMMY UNIVERSAL ANALYTICS', data);
			});
		});
	</script>
	<!-- End Universal Analyitcs -->
{{else}}
	<script>$script.done('ua');</script>
{{/unless}}
