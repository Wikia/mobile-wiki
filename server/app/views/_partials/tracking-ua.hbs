{{#unless queryParams.noexternals}}
	<script>
		// Create initial ga() function and set initial execution time
		// This code is based on code from Google that shows one how to include UA Tracking.
		(function(win){
			win['GoogleAnalyticsObject'] = 'ga';
			win['ga'] = win['ga'] || function () {
				(win['ga'].q = win['ga'].q || []).push(arguments);
			};
			win['ga'].l = 1 * new Date();
		})(window);

		// Load UA
		$script(['{{server.gaUrl}}'], 'ua');
		// load window.trackingDimensions
		$script(['/wikia.php?controller=MercuryApi&method=getTrackingDimensions'], 'dimensions');

		// when everything loads, track pageview
		$script.ready(['dimensions', 'ua', 'optimizely'], function() {
			var ua = M.tracker.UniversalAnalytics;

//			ua.initialize(window.trackingDimensions);
			ua.initialize();
			ua.updateTrackedUrl();
			ua.trackPageView();
		});
	</script>
{{else}}
	<script>$script.done('ua');</script>
{{/unless}}
