{{#unless queryParams.noexternals}}
	<script>
		// Load UA async and flag $script when ready
		$script(['{{server.gaUrl}}'], 'ua');

    // when optimizely loads, initialize trackers and track initial pageview
    $script.ready(['optimizely'], function() {
			$.getJSON('/wikia.php', {
				controller: 'MercuryApi',
				method: 'getTrackingDimensions',
				title: M.prop('pageTitle'),
				format: 'json',
				cb: Date.now
			}, function(data) {
					if (data && data.dimensions) {
						var ua = M.tracker.UniversalAnalytics,
							dimensions = [];

						$.each(data.dimensions, function(index, value) {
							dimensions[index] = value;
						});
						if (ua.initialize(dimensions)) {
							ua.trackPageView();
						}
					}
			});
    });
	</script>
{{/unless}}
