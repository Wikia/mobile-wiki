<section class="article-body">
	{{#unless preview}}
		{{> wiki-page-header}}
	{{/unless}}
	{{#if articlePage.data.nsSpecificContent.media}}
		{{> file-thumbnail media=articlePage.data.nsSpecificContent.media}}
	{{/if}}

	{{#if articleContent}}
		<article class="article-content" id="preloadedContent">
			{{{articleContent}}}
		</article>
	{{/if}}
</section>

{{#if articlePage.data.nsSpecificContent.fileUsageList}}
	{{> file-usage-list
		fileUsageList=articlePage.data.nsSpecificContent.fileUsageList
		fileUsageListSeeMoreUrl=articlePage.data.nsSpecificContent.fileUsageListSeeMoreUrl
	}}
{{/if}}

{{#if articlePage.data.nsSpecificContent.membersGrouped}}
	{{> category-members-grouped model=articlePage.data.nsSpecificContent}}
{{else if articlePage.data.nsSpecificContent.members.sections}}
{{! TODO Remove after XW-2583 is released }}
	{{> category-sections categorySections=articlePage.data.nsSpecificContent.members.sections}}
{{/if}}

{{> article-footer/index model=articlePage.data}}
